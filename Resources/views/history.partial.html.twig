{% for commit in history %}
    <div class="card ddr-gitki-history-entry">
        <h2 class="card-header ddr-gitki-history-hash">{{ commit.hash }}</h2>
        <div class="card-body ddr-gitki-history-message">
            {{ commit.message }}
        </div>
        <div class="card-footer">
            <div class="row">
                <div class="col-md-6 ddr-gitki-history-committer">{{ commit.committer }} &lt;{{ commit.eMail }}&gt;</div>
                <div class="col-md-6 text-right ddr-gitki-history-date">{{ commit.timeStamp|date("Y/m/d H:i:s") }}</div>
            </div>
        </div>
    </div>
{% endfor %}
