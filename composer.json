{
    "name": "dontdrinkandroot/gitki-bundle",
    "description": "Symfony Bundle that allows you to easily integrate a git based wiki into your project",
    "keywords": [
        "git",
        "wiki",
        "markdown",
        "symfony",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philip Washington Sorst",
            "email": "philip@sorst.net",
            "homepage": "https://sorst.net"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/finder": "^4.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/twig-bridge": "^4.0",
        "symfony/asset": "^4.0",
        "symfony/templating": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/validator": "^4.0",
        "dontdrinkandroot/utils": "0.5.x-dev",
        "cpliakas/git-wrapper": "^2.0",
        "league/commonmark": "^0.17",
        "webuni/commonmark-table-extension": "^0.8"
    },
    "require-dev": {
        "symfony/console": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/yaml": "^4.0",
        "phpunit/phpunit": "^6.0",
        "elasticsearch/elasticsearch": "^6.0",
        "scrutinizer/ocular": "^1.1",
        "liip/functional-test-bundle": "^2.0-alpha"
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Include this if you want the wiki to be searchable",
        "braincrafted/bootstrap-bundle": "Style forms in a bootstrap compatible style"
    },
    "autoload": {
        "psr-4": {
            "Dontdrinkandroot\\GitkiBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
